{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "quiz-app",
    "insertData.js",
    "app/(auth)/forgot-password",
    "app/(auth)/reset-password",
    "prisma/seed.js"
, "components/QuizDetails/QuestionOptions", "components/QuizDetails/QuestionOptions", "components/QuizDetails/Questions", "components/QuizDetails/QuestionOption"  ],
  "exclude": ["node_modules"]
}
